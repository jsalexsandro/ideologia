generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model User {
  name      String
  username  String @unique
  id        String @unique @default(cuid())
  email     String @unique
  type      String
  bio       String 
  level     String
  password  String
  avatarUrl String
}
